{
  "name": "rumahberbagi-app",
  "description": "Kelas Rumah Berbagi",
  "author": "Zain Fathoni <rb@zainf.dev>",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "next dev",
    "build": "node -p \"parseInt(process.versions.node.split('.')[0], 10)\" | awk '{if ($1 >= 17) exit 0; exit 1}' && NODE_OPTIONS=--openssl-legacy-provider next build || next build",
    "start": "next start",
    "lint": "eslint . --cache-location node_modules/.cache/eslint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "0.3.1",
    "firebase": "8.3.2",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@heroicons/react": "1.0.1",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/jit": "0.1.4",
    "@tailwindcss/typography": "0.4.0",
    "@types/react": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "autoprefixer": "10.2.5",
    "eslint": "7.23.0",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-react": "7.23.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "postcss": "8.2.8",
    "prettier": "2.2.1",
    "stylelint": "13.12.0",
    "stylelint-config-recommended": "4.0.0",
    "tailwindcss": "2.0.4",
    "typescript": "4.3.4"
  },
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  },
  "lint-staged": {
    "*.js": "eslint --fix --cache --cache-location node_modules/.cache/eslint",
    "*.css": "stylelint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
